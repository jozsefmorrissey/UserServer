package com.userSrvc.client.error;

public enum ERROR_MSGS {
	EMAIL_ALREADY_REGISTERED("Email address already registered."),
	MISLEADING("Misleading"),
	UNPRODUCTIVE("Unproductive"),
	AVOID("Avoid");
	
	
	private String value;

	EVENT_FLAGS(String value) {
	        this.value = value;
	    }

	public String value()
	{
		return value;
	}

	@Override
	public String toString()
	{
		return value;
	}
}
