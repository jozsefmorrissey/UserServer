package com.userSrvc.client.error;

public enum ERROR_MSGS {
	EMAIL_ALREADY_REGISTERED("Email address already registered.")
	
	private String value;

	ERROR_MSGS(String value) {
	        this.value = value;
	    }

	public String value()
	{
		return value;
	}

	@Override
	public String toString()
	{
		return value;
	}
}
